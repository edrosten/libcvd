echo
echo Missing options for $host_os:

echo $options $all_options $os_options |  $AWK 'BEGIN{RS=" "}{a[$1]++}END{for(i in a)if(a[i]==1)printf(i" "); print""}'


echo
echo Programs
echo $progs | sed -e's!progs/!!g'
echo
echo Debug:
